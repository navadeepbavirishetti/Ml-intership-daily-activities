   "source": [
    "## IQR method"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "1a2ee309-dac4-4c73-9685-4e4f73ee7cd9",
   "metadata": {},
   "outputs": [],
   "source": [
    "Q1 = data.quantile(0.25)\n",
    "Q3 = data.quantile(0.75)\n",
    "\n",
    "\n",
    "IQR = Q3 - Q1\n",
    "\n",
    "\n",
    "LB = Q1 - 1.5 * IQR\n",
    "UB = Q3 + 1.5 * IQR\n",
    "\n",
    "outliers = data[(data < (LB)) | (data > (UB))]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
